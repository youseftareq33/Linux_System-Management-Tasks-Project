Q1:

grep -i "_Mohammad" prj2_passwd_ | grep "/bin/zsh" | cut -d: -f1

---------------------------------------------------------------------------------------
Q2:

grep ^u1 prj2_passwd_ | sort -r -k3 -t: -n | grep -i '/comp322' | cut -d: -f5 | cut -d'_' -f1 | tr "a-z" "A-Z" | sort -u

---------------------------------------------------------------------------------------
Q3:

grep '/bin/sh' prj2_passwd_ | cut -d: -f5 | tr '_' ' ' | tr 'a-z' 'A-Z' > sh_shellusers

---------------------------------------------------------------------------------------
Q4:

head -20 prj2_passwd_ | tail -6 | cut -d: -f5 | tr '_' ' ' | tr 'a-z' 'A-Z' | sort -r > user.txt

---------------------------------------------------------------------------------------
Q5:

grep -i '_[a-o]' prj2_passwd_ | cut -d: -f5 | cut -d'_' -f1 | sort -u > a_oSorted.txt

---------------------------------------------------------------------------------------
Q6:

grep -v '/bin/zsh' prj2_passwd_ | cut -d: -f5 | tr 'a-z' 'A-Z' | sort -r

---------------------------------------------------------------------------------------
Q7:

grep ^u116 prj2_passwd_| sort -k3 -t: -n | cut -d: -f5 | cut -d'_' -f2

---------------------------------------------------------------------------------------
Q8:

grep -L passwd /etc/* 2>outerror|more

---------------------------------------------------------------------------------------
Q9:

ls /etc | cut -c1 | grep -i '[a-k]' | wc -l

---------------------------------------------------------------------------------------
Q10:

grep '/bin/ksh' prj2_passwd_ | cut -d: -f5 | sed 's/^\(.\).*_\(.\).*/\1\2/'
